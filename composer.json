{
  "name": "bartlomiejbeta/api-scope-bundle",
  "description": "Simple API query string scope recognizer",
  "type": "symfony-bundle",
  "keywords": [
    "api",
    "scope",
    "serialization"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Bart≈Çomiej Beta",
      "homepage": "https://github.com/bartlomiejbeta"
    }
  ],
  "autoload": {
    "psr-4": {
      "BartB\\APIScopeBundle\\": ""
    }
  },
  "require": {
    "php": "^7.0",
    "sensio/framework-extra-bundle": "^5.1|^5.2",
    "symfony/config": "^3.0|^4.0",
    "symfony/dependency-injection": "^3.0|^4.0",
    "symfony/event-dispatcher": "^3.0|^4.0",
    "symfony/http-foundation": "^3.0|^4.0",
    "symfony/security-core": "^3.0|^4.0",
    "doctrine/annotations": "~1.0",
    "doctrine/common": "~2.2",
    "symfony/yaml": "~3.4|~4.0"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "~2.7",
    "phpunit/phpunit": "^5.4|^6.0"
  },
  "config": {
    "bin-dir": "bin",
    "sort-packages": true
  }
}
